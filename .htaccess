<IfModule mod_php5.c>
php_value upload_max_filesize 513M
php_value post_max_size 513M
php_value memory_limit 512M
<IfModule env_module>
  SetEnv htaccessWorking true
</IfModule>
</IfModule>

<ifModule mod_headers.c>
    Header set Connection keep-alive
</ifModule>

AddDefaultCharset utf-8
Options -Indexes
